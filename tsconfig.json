{
    "compilerOptions": {
        "baseUrl": ".",
        "target": "ESNext",
        "module": "ESNext",
        "moduleResolution": "bundler",
        "strict": true,
        "skipLibCheck": true,
        "declaration": true,
        "declarationMap": true,
        "sourceMap": true,
        "composite": true,
        "isolatedModules": true,
        "jsx": "react-jsx"
    },
    "exclude": [
        "node_modules",
        "dist"
    ]
}